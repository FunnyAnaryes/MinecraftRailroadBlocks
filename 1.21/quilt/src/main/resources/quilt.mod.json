{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${maven_group}",
    "id": "${mod_id}",
    "version": "${mod_version}",
    "metadata": {
      "name": "${mod_name}",
      "description": "${mod_description}",
      "contributors": {
        "Interurban Dev": "Author",
        "lonelyicer": "Translator"
      },
      "icon": "assets/railroadblocksaddon/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "dev.interurban.quilt.RailroadBlocksQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": "${minecraft_version_range}"
      },
      {
        "id": "architectury",
        "version": "${architectury_version_range}"
      }
    ]
  }
}
